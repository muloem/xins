<?xml version="1.0" encoding="US-ASCII" ?>
<!DOCTYPE log PUBLIC "-//XINS//DTD XINS Logdoc 1.0//EN" "http://www.xins.org/dtd/log_1_0.dtd">
<!--
 -*- mode: Fundamental; tab-width: 4; -*-
 ex:ts=4

 $Id$
-->

<log>
	<translation-bundle locale="en_US" />

	<group id="trace" name="Tracing of constructors, class functions and instance methods">
		<entry id="2000" level="DEBUG">
			<description>Entering constructor.</description>
			<param name="className" type="text" nullable="false" />
			<param name="detail"    type="text" nullable="true" />
		</entry>
		<entry id="2001" level="DEBUG" exception="true">
			<description>Returning from constructor with exception.</description>
			<param name="className" type="text" nullable="false" />
			<param name="detail"    type="text" nullable="true" />
		</entry>
		<entry id="2002" level="DEBUG">
			<description>Returning from constructor successfully.</description>
			<param name="className" type="text" nullable="false" />
			<param name="detail"    type="text" nullable="true" />
		</entry>
		<entry id="2003" level="DEBUG">
			<description>Entering method.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
		<entry id="2004" level="DEBUG" exception="true">
			<description>Returning from method with exception.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
		<entry id="2005" level="DEBUG">
			<description>Returning from method successfully.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
	</group>

	<group id="lowlevel" name="Low-level generic issues">
		<entry id="2050" level="ERROR">
			<description>Programming error detected.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
		<entry id="2051" level="ERROR" exception="true">
			<description>Ignoring exception.</description>
			<param name="exceptionClass"   type="text" nullable="false" />
			<param name="exceptionMessage" type="text" nullable="true"  />
			<param name="description"      type="text" nullable="true"  />
		</entry>
		<entry id="2052" level="ERROR" exception="true">
			<description>Caught unexpected exception.</description>
			<param name="throwingClass"  type="text" nullable="false" />
			<param name="throwingMethod" type="text" nullable="false" />
		</entry>
	</group>

	<group id="call" name="XINS service calling">
		<entry id="2100" level="INFO">
			<description>Calling XINS service.</description>
			<param name="url"               type="text"         nullable="false" />
			<param name="function"          type="text"         nullable="false" />
			<param name="parameters"        type="serializable" nullable="true"  />
			<param name="totalTimeOut"      type="int32"        nullable="false" />
			<param name="connectionTimeOut" type="int32"        nullable="false" />
			<param name="socketTimeOut"     type="int32"        nullable="false" />
		</entry>
		<entry id="2101" level="INFO">
			<description>XINS service called.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
		</entry>
		<entry id="2102" level="NOTICE" exception="false">
			<description>Unknown host error while calling XINS service.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
		</entry>
		<entry id="2103" level="NOTICE">
			<description>Connection refused while calling XINS service.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
		</entry>
		<entry id="2104" level="NOTICE">
			<description>Connection time-out while calling XINS service.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
			<param name="timeOut"    type="int32"        nullable="false" />
		</entry>
		<entry id="2105" level="NOTICE">
			<description>Socket time-out while calling XINS service.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
			<param name="timeOut"    type="int32"        nullable="false" />
		</entry>
		<entry id="2106" level="NOTICE">
			<description>Total call time-out while calling XINS service.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
			<param name="timeOut"    type="int32"        nullable="false" />
		</entry>
		<entry id="2107" level="INFO">
			<description>XINS result received, status code is considered acceptable.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
			<param name="code"       type="int32"        nullable="false" />
		</entry>
		<entry id="2108" level="INFO">
			<description>XINS result received, status code is considered unacceptable.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
			<param name="code"       type="int32"        nullable="false" />
		</entry>
		<entry id="2109" level="NOTICE" exception="true">
			<description>Unrecognized I/O error while calling XINS service.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
		</entry>
		<entry id="2110" level="NOTICE" exception="false">
			<description>Invalid result received from XINS service.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
			<param name="detail"     type="text"         nullable="false" />
		</entry>
		<entry id="2111" level="INFO" exception="false">
			<description>Successful XINS result received.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
		</entry>
		<entry id="2112" level="INFO" exception="false">
			<description>Unsuccessful XINS result received.</description>
			<param name="url"        type="text"         nullable="false" />
			<param name="function"   type="text"         nullable="false" />
			<param name="parameters" type="serializable" nullable="true"  />
			<param name="duration"   type="int64"        nullable="false" />
			<param name="errorCode"  type="text"         nullable="false" />
		</entry>
	</group>

	<group id="parse" name="XINS call result parsing">
		<entry id="2200" level="NOTICE">
			<description>Root element is not "result".</description>
			<param name="elementName" type="text" nullable="false" />
		</entry>
		<entry id="2201" level="INFO">
			<description>Found output parameter with name nor value.</description>
		</entry>
		<entry id="2202" level="INFO">
			<description>Found output parameter without name.</description>
			<param name="value" type="text" nullable="false" />
		</entry>
		<entry id="2203" level="INFO">
			<description>Found output parameter without value.</description>
			<param name="name" type="text" nullable="false" />
		</entry>
		<entry id="2204" level="DEBUG">
			<description>Parsed output parameter.</description>
			<param name="name"  type="text" nullable="false" />
			<param name="value" type="text" nullable="false" />
		</entry>
		<entry id="2205" level="NOTICE" exception="true">
			<description>Unable to convert the specified character string to XML.</description>
			<param name="detail" type="text" nullable="true" />
		</entry>
		<entry id="2206" level="INFO">
			<description>Ignoring unrecognized element at root level.</description>
			<param name="type" type="text" nullable="false" />
		</entry>
	</group>
</log>
