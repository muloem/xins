<?xml version="1.0" encoding="US-ASCII" ?>
<!DOCTYPE log PUBLIC "-//XINS//DTD XINS Logdoc 1.0//EN" "http://xins.sourceforge.net/dtd/log_1_0.dtd">
<!--
 -*- mode: Fundamental; tab-width: 4; -*-
 ex:ts=4

 $Id$
-->

<log default-locale="en_US" domain="org.xins.server">

	<translation-bundle locale="en_US" />

	<group id="client" name="Client logdoc">
		<entry id="1000" level="ERROR">
			<description>Unable to convert the input from the specified reader to XML.</description>
			<param name="detail" />
		</entry>
		<entry id="1001" level="ERROR">
			<description>Found element with an empty key and empty value.</description>
			<param name="element_name" nullable="false" />
		</entry>
		<entry id="1002" level="ERROR">
			<description>Found element with an empty key.</description>
			<param name="element_name" nullable="false" />
		</entry>
		<entry id="1003" level="ERROR">
			<description>Found element with an attribute but an empty value.</description>
			<param name="element_name" nullable="false" />
			<param name="key_attribute" nullable="false" />
			<param name="key" nullable="false" />
		</entry>
		<entry id="1004" level="DEBUG">
			<description>Found element with an attribute and a value.</description>
			<param name="element_name" />
			<param name="key_attribute" />
			<param name="key" />
			<param name="value" />
		</entry>
		<entry id="1005" level="ERROR">
			<description>Unable to convert the specified character string to XML.</description>
			<param name="detail" />
		</entry>
		<entry id="1006" level="ERROR">
			<description>The returned XML is invalid. The type of the root element is not "result".</description>
			<param name="element_name" nullable="false" />
		</entry>
		<entry id="1007" level="ERROR" exception="true">
			<description>Caught exception while releasing HTTP connection after request failed.</description>
			<param name="class_name" nullable="false" />
		</entry>
		<entry id="1008" level="ERROR">
			<description>Not OK HTTP code.</description>
			<param name="code" type="int32" nullable="false" />
		</entry>
		<entry id="1009" level="ERROR">
			<description>Failed to read the response body.</description>
		</entry>
		<entry id="1010" level="ERROR" exception="true">
			<description>Unexpected exception caught while calling doCall(org.xins.util.service.CallRequest).</description>
			<param name="class_name" nullable="false" />
		</entry>
	</group>

</log>
