<log default-locale="en_US">

	<messageset id="en_US" />
	<messageset id="nl_NL" />

	<entry id="0" level="DEBUG" category="org.xins.server.lifespan.bootstrap">
		<description>State change in APIServlet instance.</description>
		<param name="oldState" />
		<param name="newState" />
	</entry>
	<entry id="100" level="DEBUG" category="org.xins.server.lifespan.bootstrap">
		<description>Bootstrapping XINS/Java Server Framework.</description>
	</entry>
	<entry id="101" level="ERROR" category="org.xins.server.lifespan.bootstrap">
		<description>Unable to bootstrap XINS/Java Server Framework due to unexpected state.</description>
		<param name="state" />
	</entry>
	<entry id="102" level="FATAL" category="org.xins.server.lifespan.bootstrap">
		<description>Unable to bootstrap XINS/Java Server Framework due to application server behaving unexpectedly.</description>
		<param name="detail" />
	</entry>
	<entry id="103" level="WARNING" category="org.xins.server.lifespan.bootstrap">
		<description>Application server implements unexpected version of Java Servlet API Specification.</description>
		<param name="actual_version" />
		<param name="expected_version" />
	</entry>
	<entry id="104" level="ERROR" category="org.xins.server.lifespan.bootstrap">
		<description>Unable to get system property that defines runtime configuration file to use, due to a security restriction.</description>
		<param name="propertyName" />
		<param name="exceptionClass" />
		<param name="exceptionMessage" />
	</entry>
	<entry id="105" level="FATAL" category="org.xins.server.lifespan.bootstrap">
		<description>System property that defines runtime configuration file to use is not set.</description>
		<param name="propertyName" />
	</entry>
	<entry id="106" level="FATAL" category="org.xins.server.lifespan.bootstrap">
		<description>API class name not set in build property.</description>
		<param name="propertyName" />
	</entry>
	<entry id="200" level="NOTICE" category="org.xins.server.lifespan.init">
		<description>XINS/Java Server Framework initialized or re-initialized.</description>
	</entry>
	<entry id="201" level="INFO" category="org.xins.server.lifespan.init">
		<description>About to change locale for the log messages.</description>
		<param name="oldLocale" />
		<param name="newLocale" />
	</entry>
	<entry id="202" level="NOTICE" category="org.xins.server.lifespan.init">
		<description>Changed locale for the log messages.</description>
		<param name="oldLocale" />
		<param name="newLocale" />
	</entry>
	<entry id="203" level="ERROR" category="org.xins.server.lifespan.init">
		<description>Failed to change the locale for the log messages.</description>
		<param name="currentLocale" />
		<param name="failedLocale" />
	</entry>
</log>
