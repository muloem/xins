<?xml version="1.0" encoding="US-ASCII" ?>
<!DOCTYPE log PUBLIC "-//XINS//DTD XINS Logdoc 1.0//EN" "http://xins.sourceforge.net/dtd/log_1_0.dtd">
<!--
 -*- mode: Fundamental; tab-width: 4; -*-
 ex:ts=4

 $Id$
-->

<log>
	<translation-bundle locale="en_US" />

	<group id="lowlevel" name="Low-level (tracing, programming errors)">
		<entry id="3000" level="DEBUG">
			<description>Entering constructor.</description>
			<param name="className" type="text" nullable="false" />
			<param name="detail"    type="text" nullable="true" />
		</entry>
		<entry id="3001" level="DEBUG" exception="true">
			<description>Returning from constructor with exception.</description>
			<param name="className" type="text" nullable="false" />
			<param name="detail"    type="text" nullable="true" />
		</entry>
		<entry id="3002" level="DEBUG">
			<description>Returning from constructor successfully.</description>
			<param name="className" type="text" nullable="false" />
			<param name="detail"    type="text" nullable="true" />
		</entry>
		<entry id="3003" level="DEBUG">
			<description>Entering method.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
		<entry id="3004" level="DEBUG" exception="true">
			<description>Returning from method with exception.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
		<entry id="3005" level="DEBUG">
			<description>Returning from method successfully.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
		<entry id="3006" level="ERROR">
			<description>Programming error detected.</description>
			<param name="className"  type="text" nullable="false" />
			<param name="methodName" type="text" nullable="false" />
			<param name="detail"     type="text" nullable="true" />
		</entry>
	</group>

	<group id="collections" name="Generic collection classes" />

	<group id="collections.expiry" name="Expiry-related collection classes">
		<entry id="3400" level="DEBUG">
			<description>Tick processed.</description>
			<param name="folderName" type="text"  nullable="false" />
			<param name="expired"    type="int32" nullable="false" />
			<param name="remaining"  type="int32" nullable="false" />
		</entry>
		<entry id="3404" level="DEBUG">
			<description>Associating expiry folder with expiry stategy thread.</description>
			<param name="folder"   type="text" nullable="false" />
			<param name="strategy" type="text" nullable="false" />
		</entry>
		<entry id="3405" level="DEBUG">
			<description>Expiry strategy thread started.</description>
			<param name="name" type="text" nullable="false" />
		</entry>
		<entry id="3406" level="DEBUG">
			<description>Expiry strategy thread stopped.</description>
			<param name="name" type="text" nullable="false" />
		</entry>
	</group>

	<group id="http" name="HTTP-related" />

	<group id="io" name="General I/O">
		<entry id="3200" level="INFO">
			<description>Starting file watch thread.</description>
			<param name="file" type="text" nullable="false" />
		</entry>
		<entry id="3201" level="INFO">
			<description>Changing file watch interval.</description>
			<param name="file"        type="text"  nullable="false" />
			<param name="oldInterval" type="int32" nullable="false" />
			<param name="newInterval" type="int32" nullable="false" />
		</entry>
		<entry id="3202" level="INFO">
			<description>Stopping file watch thread.</description>
			<param name="file" nullable="false" />
		</entry>
	</group>

	<group id="service" name="Service calling framework">
		<entry id="3303" level="ERROR" exception="true">
			<description>URL is malformed.</description>
			<param name="url" type="text" nullable="false" />
		</entry>
		<entry id="3312" level="DEBUG">
			<description>Call to a specific target succeeded.</description>
			<param name="url" type="text" nullable="false" />
		</entry>
		<entry id="3313" level="WARNING">
			<description>Call to a specific target failed.</description>
			<param name="url" type="text" nullable="false" />
		</entry>
		<entry id="3314" level="ERROR">
			<description>Call completely failed.</description>
			<!-- TODO: Include call (request) ID ? -->
		</entry>
		<entry id="3315" level="INFO">
			<description>No more targets and no fail-over</description>
		</entry>
		<entry id="3316" level="INFO">
			<description>No more targets, but fail-over would be allowed</description>
		</entry>
		<entry id="3317" level="INFO">
			<description>More targets available, but fail-over is not allowed</description>
		</entry>
		<entry id="3318" level="INFO">
			<description>More targets available and fail-over is allowed</description>
		</entry>
	</group>

	<group id="text" name="Text">
		<entry id="3205" level="DEBUG">
			<description>Increasing capacity of FastStringBuffer instance.</description>
			<param name="currentCapacity" type="int32" nullable="false" />
			<param name="newCapacity"     type="int32" nullable="false" />
		</entry>
	</group>

	<group id="threads" name="Threading">
		<entry id="3420" level="ERROR">
			<description>Unable to add Thread to Doorman queue due to a time-out.</description>
			<param name="className"        type="text"  nullable="false" />
			<param name="thread"           type="text"  nullable="false" />
			<param name="maxQueueWaitTime" type="int64" nullable="false" />
		</entry>
	</group>

	<group id="types" name="Types" />
</log>
