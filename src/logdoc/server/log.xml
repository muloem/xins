	<group id="runtime.tx" name="Transaction logging">
		<entry id="1540" level="INFO">
			<description>Function called. This entry differs from 1541 in that it logs all input and output parameters, while 1541 does not.</description>
			<param name="start"        type="serializable" nullable="false" />
			<param name="ip"           type="text"         nullable="false" />
			<param name="functionName" type="text"         nullable="false" />
			<param name="callID"       type="int32"        nullable="false" />
			<param name="duration"     type="int64"        nullable="false" />
			<param name="errorCode"    type="text"         nullable="false" />
			<param name="inParams"     type="serializable" nullable="false" />
			<param name="outParams"    type="serializable" nullable="false" />
		</entry>
		<entry id="1541" level="INFO">
			<description>Function called. This entry differs from 1540 in that it does not log all input and output parameters, while 1540 does.</description>
			<param name="start"        type="serializable" nullable="false" />
			<param name="ip"           type="text"         nullable="false" />
			<param name="functionName" type="text"         nullable="false" />
			<param name="callID"       type="int32"        nullable="false" />
			<param name="duration"     type="int64"        nullable="false" />
			<param name="errorCode"    type="text"         nullable="false" />
		</entry>
	</group>

	<group id="runtime.acl" name="Access rule matching">
		<entry id="1550" level="INFO">
			<description>Access rule matches. Allowing function call.</description>
			<param name="ip" nullable="false" />
			<param name="function" nullable="false" />
			<param name="index" type="int32" nullable="false" />
			<param name="rule" nullable="false" />
		</entry>
		<entry id="1551" level="INFO">
			<description>Access rule matches. Denying function call.</description>
			<param name="ip" nullable="false" />
			<param name="function" nullable="false" />
			<param name="index" type="int32" nullable="false" />
			<param name="rule"  nullable="false" />
		</entry>
		<entry id="1552" level="DEBUG">
			<description>Access rule does not match.</description>
			<param name="ip" nullable="false" />
			<param name="function" nullable="false" />
			<param name="index" type="int32" nullable="false" />
			<param name="rule"  nullable="false" />
		</entry>
		<entry id="1553" level="INFO">
			<description>No access rule matched. Denying function call.</description>
			<param name="ip" nullable="false" />
			<param name="function" nullable="false" />
		</entry>
	</group>

