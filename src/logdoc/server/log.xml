			<param name="buildVersion" />
		</entry>
		<entry id="1219" level="DEBUG">
			<description>Bootstrapping manageable object.</description>
			<param name="api" />
			<param name="className" />
		</entry>
		<entry id="1220" level="DEBUG">
			<description>Bootstrapped manageable object.</description>
			<param name="api" />
			<param name="className" />
		</entry>
		<entry id="1221" level="FATAL">
			<description>Manageable object could not be bootstrapped due to a missing required property.</description>
			<param name="api" />
			<param name="className" />
			<param name="propertyName" />
		</entry>
		<entry id="1222" level="FATAL">
			<description>Manageable object could not be bootstrapped due to an invalid property value.</description>
			<param name="api" />
			<param name="className" />
			<param name="propertyName" />
			<param name="propertyValue" />
		</entry>
		<!-- TODO: <reserved id="1223" /> -->
		<entry id="1224" level="FATAL" exception="true">
			<description>Manageable object could not be bootstrapped.</description>
			<param name="api" />
			<param name="className" />
			<param name="message" />
		</entry>
		<entry id="1225" level="DEBUG">
			<description>Adding manageable object to API.</description>
			<param name="api" />
			<param name="className" />
		</entry>
		<entry id="1226" level="DEBUG">
			<description>Manageable object added to API.</description>
			<param name="api" />
			<param name="className" />
		</entry>
		<entry id="1227" level="DEBUG">
			<description>Bootstrapping function.</description>
			<param name="api" />
			<param name="function" />
		</entry>
		<entry id="1228" level="DEBUG">
			<description>Bootstrapped function.</description>
			<param name="api" />
			<param name="function" />
		</entry>
		<entry id="1229" level="FATAL">
			<description>Function could not be bootstrapped due to a missing required property.</description>
			<param name="api" />
			<param name="function" />
			<param name="propertyName" />
		</entry>
		<entry id="1230" level="FATAL">
			<description>Function could not be bootstrapped due to an invalid property value.</description>
			<param name="api" />
			<param name="function" />
			<param name="propertyName" />
			<param name="propertyValue" />
		</entry>
		<!-- TODO: <reserved id="1231" /> -->
		<entry id="1232" level="FATAL" exception="true">
			<description>Function could not be bootstrapped.</description>
			<param name="api" />
			<param name="function" />
			<param name="message" />
		</entry>
		<entry id="1233" level="INFO">
			<description>XINS/Java Server Framework is bootstrapped.</description>
			<param name="version" />
		</entry>
		<entry id="1234" level="WARNING">
			<description>XINS/Java Server Framework version is different from XINS/Java Common Library version.</description>
			<param name="serverVersion" />
			<param name="commonVersion" />
		</entry>
		<entry id="1235" level="WARNING">
			<description>XINS/Java Server Framework version does not indicate a production release.</description>
			<param name="version" />
		</entry>
