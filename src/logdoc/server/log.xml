			<description>State change in APIServlet instance.</description>
			<param name="oldState" />
			<param name="newState" />
		</entry>
		<entry id="1101" level="ERROR">
			<description>State change invalid.</description>
			<param name="oldState" />
			<param name="newState" />
		</entry>
	</group>

	<group id="lifespan.bootstrap" name="Bootstrapping">
